package com.xworkz.DTO.brtc.brtcDTO;

public class BrtcDTO {
	private String busNo;
	private String destinationPoint;
	private String sourcePoint;
	private String route;
	
	public BrtcDTO() {
		
	}

	public String getBusNo() {
		return busNo;
	}

	public void setBusNo(String busNo) {
		this.busNo = busNo;
	}

	public String getDestinationPointString() {
		return destinationPoint;
	}

	public void setDestinationPointString(String destinationPointString) {
		this.destinationPoint = destinationPointString;
	}

	public String getSourcePointString() {
		return sourcePoint;
	}

	public void setSourcePointString(String sourcePointString) {
		this.sourcePoint = sourcePointString;
	}

	public String getRoute() {
		return route;
	}

	public void setRoute(String route) {
		this.route = route;
	}
	
	@Override
	public String toString() {
		return "Bus No = " +busNo+ " ,Route = "+route+" ,From = "+sourcePoint+ ", To = "+destinationPoint;
	}

}
